            </geometry>
            <material name="link2_color">
                <color rgba="0.9 0.9 0.9 1.0"/>
            </material>
        </visual>
    </link>

    <!-- link 4-->
        <!-- obrot wokol osi z -->
    <joint name="joint4" type="revolute">
        <parent link="link3"/>
        <child link="link4"/>
        <origin xyz="0 0 0.05" rpy = "0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-3.14159" upper="3.14159" effort="100" velocity="1.0"/>
    </joint>

    <link name="link4">
