        <parent link="link5"/>
        <child link="link6"/>
        <origin xyz="0 0 0.05" rpy = "0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-3.14159" upper="3.14159" effort="100" velocity="1.0"/>
    </joint>
    
    <link name="link6">
        <inertial>
            <mass value="0.5"/>
